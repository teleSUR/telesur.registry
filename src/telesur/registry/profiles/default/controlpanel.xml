<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="telesur.registry">

    <configlet
        title="Disqus settings"
        action_id="telesur.registry.disqus"
        appId="telesur.registry"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@telesur-disqus-controlpanel"
        icon_expr="string:${portal_url}/++resource++telesur.registry.images/disqus-icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
